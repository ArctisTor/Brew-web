{
  "name": "brew-web",
  "version": "1.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config client/assets/config/proxy_config.json",
    "prebuild": "npm --no-git-tag-version version patch",
    "build": "ng build --production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.0.3",
    "@angular/cdk": "~13.0.0",
    "@angular/common": "^13.0.3",
    "@angular/compiler": "^13.0.3",
    "@angular/core": "^13.0.3",
    "@angular/forms": "^13.0.3",
    "@angular/material": "^13.1.2",
    "@angular/platform-browser": "^13.0.3",
    "@angular/platform-browser-dynamic": "^13.0.3",
    "@angular/router": "^13.0.3",
    "@googlemaps/js-api-loader": "^1.13.2",
    "@types/arcgis-js-api": "^4.22.0",
    "@types/google.maps": "^3.47.2",
    "bootstrap": "^5.1.3",
    "esri-loader": "^3.4.0",
    "ngx-toastr": "^14.2.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.4",
    "@angular/cli": "^13.0.4",
    "@angular/compiler-cli": "^13.0.3",
    "@types/jasmine": "^3.10.3",
    "@types/node": "^12.20.42",
    "jasmine-core": "~3.10.0",
    "karma": "^6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.4.3"
  },
  "bin": "server/server.js",
  "pkg": {
    "assets": [
      "brew-web-application/**/*",
      "server/**/*"
    ],
    "targets": [
      "node12-macos-x64",
      "node10-win-x64",
      "node14-linux-arm64"
    ]
  }
}
